/* ------------------- Seccion de la gramatica ------------------------ */

S0 ::=  S0 INICIO
        | INICIO
        ;

INICIO ::=  DOLLAR CPREPORT DOLLAR
            | OPMEN HTML OPMAY OPSHTML OPMEN OPDIV HTML OPMAY
            | error OPMAY
            | TEXTOPLANO
            ;

OPSHTML ::= SENTSHTML
